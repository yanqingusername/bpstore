.case-list {}

.search {
  display: flex;
  align-items: center;
  width: 100%;
  height: 76rpx;
  padding: 0 33rpx 20rpx;
  background-color: #fff;
  box-sizing: border-box;
}
.serch-container {
  flex: 1;
  display: flex;
  align-items: center;
  height: 100%;
  padding-left: 27rpx;
  border-radius: 28rpx;
  border: 1px solid #eee;
  box-sizing: border-box;
}
.serch-container > image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 7rpx;
}
.serch-container > text {
  font-size: 24rpx;
  color: #8C96A4;
}
.search-add {
  flex-shrink: 0;
  width: 42rpx;
  height: 42rpx;
  margin-left: 34rpx;
}