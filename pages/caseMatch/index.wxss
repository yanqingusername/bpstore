.finalist {
  padding: 0 20rpx;
}
.finalist-item {
  padding: 30rpx 0;
  border-bottom: 1px solid #eee;
}
.finalist-item-title {
  display: flex;
  align-items: center;
  font-size: 32rpx;
}
.finalist-item-title > image {
  width: 26rpx;
  height: 26rpx;
  margin-right: 10rpx;
}
.finalist-item-content {
  display: flex;
  margin-top: 27rpx;
}
.finalist-item-content-left {
  font-size: 28rpx;
  color: #616E7B;
}
.hot {
  margin-top: 16rpx;
  font-size: 22rpx;
  color: #ACBBCB;
}
.finalist-item-content-right {
  flex-shrink: 0;
  width: 170rpx;
  height: 120rpx;
  margin-left: 39rpx;
}
.finalist-item-content-right > image {
  width: 100%;
  height: 100%;
  border-radius: 8rpx;
  background-color: aqua;
}

.award-winning {
  width: 100%;
  height: 1334rpx;
  position: relative;
}
.award-winning > image {
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 1;
}
.award-winning-list {
  width: 620rpx;
  position: absolute;
  left: 50%;
  top: 475rpx;
  transform: translateX(-50%);
  z-index: 2;
}
.award-winning-list-item {
  width: 100%;
  padding: 24rpx 0;
  border-radius: 18rpx;
  background: #fff;
  box-sizing: border-box;
  position: relative;
}
.award-winning-list-item > image {
  width: 50rpx;
  height: 65rpx;
  position: absolute;
  right: 36rpx;
  top: 0;
}
.award-winning-list-item-header {
  width: 100%;
  height: 48rpx;
  font-size: 22rpx;
  position: relative;
}
.award-winning-list-item-header-tag {
  display: flex;
  align-items: center;
  position: absolute;
  left: -10px;
  top: 0;
  height: 100%;
  padding: 0 24rpx;
  background-color: #3C87F3;
  color: #fff;
}
.award-winning-list-item-main {
  padding: 29rpx 15rpx 0 34rpx;
}
.award-winning-list-item-title {
  font-size: 32rpx;
  font-weight: bold;
}
.city-industry {
  display: flex;
  justify-content: space-between;
  font-size: 22rpx;
  color: #ACBBCB;
  margin-top: 27rpx;
}
.city-industry-right {
  font-size: 18.46rpx;
  color: #999999;
}
.tag-list {}
.tag-item {
  display: inline-flex;
  align-items: center;
  height: 25rpx;
  padding: 0 9rpx;
  font-size: 16rpx;
  color: #00C0C2;
  border: 1px solid #00C0C2;
  border-radius: 2rpx;
  box-sizing: border-box;
}



.btn {
  width: 100%;
  padding: 40rpx 30rpx;
  box-sizing: border-box;
  position: fixed;
  left: 0;
  bottom: 0;
}
.btn > view {
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  color: #fff;
  background-color: #2A64F5;
  border-radius: 10rpx;
}