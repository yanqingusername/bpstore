.anlie-img{
  width: 100%;
  min-height: 1200rpx;
}
.finalist {
  padding: 0 20rpx;
}
.finalist-item {
  padding: 30rpx 0;
  border-bottom: 1px solid #eee;
}
.finalist-item-title {
  display: flex;
  align-items: center;
  font-size: 32rpx;
}
.finalist-item-title > image {
  width: 26rpx;
  height: 26rpx;
  margin-right: 10rpx;
}
.finalist-item-content {
  display: flex;
  margin-top: 27rpx;
}
.finalist-item-content-left {
  font-size: 28rpx;
  color: #616E7B;
  width: 502rpx;
}
.hot {
  margin-top: 16rpx;
  font-size: 22rpx;
  color: #ACBBCB;
}
.finalist-item-content-right {
  flex-shrink: 0;
  width: 170rpx;
  height: 120rpx;
  margin-left: 39rpx;
}
.finalist-item-content-right > image {
  width: 100%;
  height: 100%;
  border-radius: 8rpx;
  background-color: aqua;
}

.award-winning {
  width: 100%;
  height: 1334rpx;
  position: relative;
}
.award-winning > image {
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 1;
}
.award-winning-list-view{
  position: absolute;
  left: 50%;
  top: 475rpx;
  transform: translateX(-50%);
  z-index: 2;
}
.award-winning-list {
  width: 620rpx;
  margin-bottom: 20rpx;
}
.award-winning-list-item {
  width: 100%;
  padding: 24rpx 0;
  border-radius: 18rpx;
  background: #fff;
  box-sizing: border-box;
  position: relative;
}
.award-winning-list-item > image {
  width: 50rpx;
  height: 65rpx;
  position: absolute;
  right: 36rpx;
  top: 0;
}
.award-winning-list-item-header {
  width: 100%;
  height: 48rpx;
  font-size: 22rpx;
  position: relative;
}
.award-winning-list-item-header-tag {
  display: flex;
  align-items: center;
  position: absolute;
  left: -10px;
  top: 0;
  height: 100%;
  padding: 0 24rpx;
  background-color: #3C87F3;
  color: #fff;
}
.award-winning-list-item-main {
  padding: 20rpx 15rpx 0 34rpx;
}
.award-winning-list-item-title {
  font-size: 32rpx;
  font-weight: bold;
}
.city-industry {
  display: flex;
  justify-content: space-between;
  font-size: 22rpx;
  color: #ACBBCB;
  margin-top: 20rpx;
}
.city-industry-right {
  font-size: 18.46rpx;
  color: #999999;
}
.tag-list {}
.tag-item {
  display: inline-flex;
  align-items: center;
  height: 25rpx;
  padding: 0 9rpx;
  font-size: 16rpx;
  color: #00C0C2;
  border: 1px solid #00C0C2;
  border-radius: 2rpx;
  box-sizing: border-box;
  margin-right: 20rpx;
}

.case-right-tag {
  display: flex;
}
.case-right-tag > view {
  display: flex;
  height: 25rpx;
  align-items: center;
  padding: 0 10rpx;
  font-size: 16rpx;
  color: #00C0C2;
  border: 1px solid #00C0C2;
  box-sizing: border-box;
  border-radius: 2rpx;
  margin-top: 6rpx;
  margin-right: 10rpx
}



.btn {
  width: 100%;
  padding: 40rpx 30rpx;
  box-sizing: border-box;
  position: fixed;
  left: 0;
  bottom: 0;
}
.btn > view {
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  color: #fff;
  background-color: #2A64F5;
  border-radius: 10rpx;
}
.cm-pop{
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.cm-pop-item{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 582rpx;
height: 370rpx;
background: #FFFFFF;
border-radius: 10rpx;
flex-direction: column;
}

.cm-pop-item-title{
  font-size: 28rpx;
font-weight: bold;
color: #666666;
}

.cm-pop-item-btn{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 502rpx;
height: 70rpx;
background: #2A64F5;
border-radius: 4rpx;
font-size: 28rpx;
font-weight: bold;
color: #FFFFFF;
margin-top: 40rpx;
}
.cm-pop-item-bomm{
  display: flex;
  align-items: center;
margin-top: 36rpx;
}
.cm-pop-item-bomm-1{
  font-size: 26rpx;
font-weight: bold;
color: #616E7B;
}
.cm-pop-item-bomm-2{
  font-size: 26rpx;
font-weight: bold;
color: #17AEF1;
}
.cm-pop-item-img{
  width: 62rpx;
  height: 62rpx;
  margin-top: 30rpx;
}

.cm-release-item{
  width: 750rpx;
height: 1002rpx;
background: #FFFFFF;
border-radius: 10rpx 10rpx 0px 0px;
}
.cm-release-item-1{
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #E8E8E8;
  padding: 32rpx 22rpx 32rpx 20rpx;
}
.cm-release-item-1-1{
  font-size: 26rpx;
font-weight: bold;
color: #616E7B;
}

.cm-release-item-1-2{
  width: 42rpx;
  height: 42rpx;
}

.scroll-release{
  height: 798rpx !important;
}

.cm-release-view{
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #E8E8E8;
  padding: 30rpx 40rpx;
}

.cm-release-left{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 42rpx;
  height: 42rpx;
}
.cm-release-left-img{
  width: 42rpx;
  height: 42rpx;
}

.cm-release-right{
  width: 600rpx;
}
.cm-release-right-1{
  font-size: 28rpx;
font-weight: bold;
color: #333333;
}

.cm-release-right-2{
  font-size: 24rpx;
color: #616E7B;
margin-top: 22rpx;
}

.cm-release-right-3{
  font-size: 22rpx;
color: #ACBBCB;
margin-top: 18rpx;
}

.cm-release-btn{
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 750rpx;
height: 100rpx;
background: #FFFFFF;
}
.cm-release-btn-left{
  display: flex;
  align-items: center;
  margin-left: 20rpx;
}
.cm-release-btn-title{
  font-size: 28rpx;
font-weight: bold;
color: #616E7B;
margin-left: 10rpx;
}
.cm-release-btn-right{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 158rpx;
height: 64rpx;
background: #3180F6;
border-radius: 32rpx;
